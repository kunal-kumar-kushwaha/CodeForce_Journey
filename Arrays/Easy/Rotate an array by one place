#include <iostream>
#include<string>
#include<set> 
#include<vector>

using namespace std ; 

int main(){
    vector<int> v ={1,2,3,4,5,6} ;
    int n =  v.size() ;


    int temp = v[0];
    for(int i = 1; i<n ; i++){
        v[i-1] = v[i] ;       
    }
    v[n-1] = temp ;

  
    for(int i = 0; i<n ; i++){
        cout << v[i] << " "  ;
    }


return 0 ;
}



