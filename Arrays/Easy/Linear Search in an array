#include <iostream>
#include<string>
#include<set> 
#include<vector>

using namespace std ; 

int main(){
    vector<int> v ={1 ,0 ,2 ,3 ,0 ,4 ,0 ,1} ;
    int n =  v.size() ;
    int key = 10 ;
    bool found = false ;
    
    for(int i = 0; i< n ; i++){
        if(v[i]== key ){
            found = true ;
            cout << i << endl ;
            break ;
        }
    }
    if(found  == false ){
        cout << "-1" ; 
    }




return 0 ;
}
